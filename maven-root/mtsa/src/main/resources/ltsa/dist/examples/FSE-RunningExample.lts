set Alph = {c1, a, g1, g2}
set Controllable = {c1, g1, g2}

fluent A = <a, Alph\{a}>
fluent G1 = <g1, Alph\{g1}>
fluent G2 = <g2, Alph\{g2}>

assert GG1 = G1
assert GG2 = G2
assert As = A

controller ||C = M~{G1}.

goal G1 = {
	safety = {}
    assume = {As}
    guarantee = {GG1, GG2}
    controllable = {Controllable}
}


M = (c1->MA),
MA = (a->{g1,g2}->MA).


